package com.bea.mbank.helper.counter;

public class PayLoanBO {
	//月份
	private int month;
	//月供
	private float monthPay;
	//月供本金
	private float monthPrincipal;
	//月供利息
	private float monthInterests;
	//剩余
	private float balance;
	//年
	private int year;
	
	/*//贷款总额
	private float loanMoney;
	//贷款月数
	private int loanMonth;
	//贷款月利率
*/	
	public int getMonth() {
		return month;
	}
	public void setMonth(int month) {
		this.month = month;
	}
	public float getMonthPay() {
		return monthPay;
	}
	public void setMonthPay(float monthPay) {
		this.monthPay = monthPay;
	}
	public float getMonthPrincipal() {
		return monthPrincipal;
	}
	public void setMonthPrincipal(float monthPrincipal) {
		this.monthPrincipal = monthPrincipal;
	}
	public float getMonthInterests() {
		return monthInterests;
	}
	public void setMonthInterests(float monthInterests) {
		this.monthInterests = monthInterests;
	}
	public float getBalance() {
		return balance;
	}
	public void setBalance(float balance) {
		this.balance = balance;
	}
	public int getYear() {
		return year;
	}
	public void setYear(int year) {
		this.year = year;
	}
	
	
}
