<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:background="#FFFFFF"
    >
    

        <RelativeLayout
            android:id="@+id/appset_Relativelayout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" 
            android:onClick="click"
             >

            <ImageView
                android:id="@+id/Setpassword_image1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="20dp"
                android:src="@drawable/ac_appset_1" />

            <TextView
                android:id="@+id/newVersionTip"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/Setpassword_image1"
                android:layout_marginBottom="22dp"
                android:layout_toRightOf="@+id/Setpassword_image1"
                android:gravity="center_vertical"
                android:text="密码设置"
                android:textSize="20sp" 
                android:textColor="#373737"
                />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/newVersionTip"
                android:layout_alignParentRight="true"
                android:layout_marginRight="20dp"
                android:src="@drawable/ac_appset_2" />
             <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content" 
                android:layout_below="@+id/Setpassword_image1"
                android:layout_alignParentRight="true" 
                android:src="@drawable/divider_line"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp" />
        </RelativeLayout> 
</LinearLayout>
