package com.bea.remote.models.crm;

/**
 * crm 身份证联网核查 基本信息
 * @author fanglinhao
 */
public class CrmPersonInfo {
	private String name;//姓名
	private String nation;//发证国家
	private String cardType;//证件类型
	private String creditCard;//身份证号
	private String Photo;//照片路径
	private String IssueOffice;//发证机关
	private String CheckResult; //00----公民身份号码与姓名一致，且存在照片
								//01----公民身份号码与姓名一致，但不存在照片
								//02----公民身份号码存在，但与姓名不匹配
								//03----公民身份号码不存在
								//04----其他错误
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getNation() {
		return nation;
	}
	public void setNation(String nation) {
		this.nation = nation;
	}
	public String getCardType() {
		return cardType;
	}
	public void setCardType(String cardType) {
		this.cardType = cardType;
	}
	public String getCreditCard() {
		return creditCard;
	}
	public void setCreditCard(String creditCard) {
		this.creditCard = creditCard;
	}
	public String getPhoto() {
		return Photo;
	}
	public void setPhoto(String photo) {
		Photo = photo;
	}
	public String getIssueOffice() {
		return IssueOffice;
	}
	public void setIssueOffice(String issueOffice) {
		IssueOffice = issueOffice;
	}
	public String getCheckResult() {
		return CheckResult;
	}
	public void setCheckResult(String checkResult) {
		CheckResult = checkResult;
	}
}
