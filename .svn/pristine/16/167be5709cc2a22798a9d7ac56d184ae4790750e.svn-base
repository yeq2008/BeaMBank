.inputtext{
	border: 0;
	width: 100%;
	font-size: 18px;
	color: #333333;
	margin-left: 0px;
	margin-right: 0px;
	padding-left: 0px;
	padding-right: 0px;
}

.inputvalue{
	border: 0;
	width: 100%;
	color: #999999;
	font-size: 18px;
	margin-left: 0px;
	margin-right: 0px;
	padding-left: 0px;
	padding-right: 0px;
}

.spantext{
	width: 100%;
	font-size: 18px;
	color: #333333;
	margin: 0px;
	padding: 0px;
}

.spanvalue{
	width: 100%;
	color: #999999;
	font-size: 18px;
	margin: 0px;
	padding: 0px;
}

.divtitle{
	margin-left: 10px;
	margin-right: 10px;
	padding-left: 0px;
	padding-right: 0px;
}

.title{
	font-size:20px;
	border: 0px;
	margin-left: 0px;
	padding-left: 0px;
}

.table{
	width: 100%;
	margin: 0px;
	padding: 0px;
}

.table tr{
	width: 100%;
	margin: 0px;
	padding: 0px;
}

.table th,td{
	width: 25%;
	height: 35px;
	margin: 0px;
	padding: 0px;
}

.titletable{
	width: 100%;
	border-collapse: collapse;
}

.titletable tr{
	width: 100%;
	height: 40px;
	border: 1px solid #e4e4e4;
}

.titletable td{
	padding-left: 10px;
}

.titletable_inputtitle{
	border: 0px;
	text-align: left;
	font-size: 18px;
	background-color: #f0f0f0;
	color: #666666;
	width: 100%;
}

.titletable_inputvalue{
	border: 0px;
	text-align: left;
	font-size: 18px;
	color: #999999;
	width: 100%;
}